package application;

import java.util.ArrayList;
import java.util.List;

public class Main {

	public static void main(String[] args) {
		List<String> list = new ArrayList<>();
		
		list.add("Maria");
		list.add("Alex");
		list.add(2, "Marco");
		list.add("Anna");
		
		System.out.println(list.size());;
		list.remove("Anna");
		
		for (String x : list) {
			System.out.println(x);
		}
	}

}
